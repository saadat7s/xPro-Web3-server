{
  "openapi": "3.0.0",
  "info": {
    "title": "Meme Token API",
    "version": "1.0.0",
    "description": "API for minting and managing meme tokens on Solana blockchain",
    "contact": {
      "name": "API Support"
    }
  },
  "servers": [
    {
      "url": "http://localhost:5000",
      "description": "Local development server"
    }
  ],
  "tags": [
    {
      "name": "Transactions",
      "description": "Unsigned transaction generation endpoints"
    },
    {
      "name": "Mint Details",
      "description": "Token balance and distribution query endpoints"
    },
    {
      "name": "AMM Pool",
      "description": "Automated Market Maker pool creation and liquidity management endpoints"
    }
  ],
  "paths": {
    "/meme/tx/initialize-protocol": {
      "post": {
        "tags": ["Transactions"],
        "summary": "Initialize protocol transaction",
        "description": "Generates an unsigned transaction to initialize the protocol",
        "operationId": "initializeProtocol",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InitializeProtocolRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Transaction created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TransactionResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/meme/tx/reset-protocol": {
      "post": {
        "tags": ["Transactions"],
        "summary": "Reset protocol transaction",
        "description": "Generates an unsigned transaction to reset the protocol",
        "operationId": "resetProtocol",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ResetProtocolRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Transaction created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TransactionResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/meme/tx/mint-meme": {
      "post": {
        "tags": ["Transactions"],
        "summary": "Mint meme token transaction",
        "description": "Generates an unsigned transaction to mint a new meme token",
        "operationId": "mintMeme",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MintMemeRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Transaction created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TransactionResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/meme/tx/create-ata": {
      "post": {
        "tags": ["Transactions"],
        "summary": "Create Associated Token Account transaction",
        "description": "Generates an unsigned transaction to create an Associated Token Account",
        "operationId": "createAta",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateAtaRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Transaction created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TransactionResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/meme/token-balance/{tokenAccountAddress}": {
      "get": {
        "tags": ["Mint Details"],
        "summary": "Get token account balance",
        "description": "Retrieves the balance of a specific token account",
        "operationId": "getTokenBalance",
        "parameters": [
          {
            "name": "tokenAccountAddress",
            "in": "path",
            "required": true,
            "description": "The public key address of the token account",
            "schema": {
              "type": "string",
              "example": "HN7cABqLq46Es1jh92dQQisAq662SmxELLLsHHe4YWrH"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Token balance retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TokenBalanceResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid token account address",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "404": {
            "description": "Token account not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/meme/meme-token-distribution/{memeId}": {
      "get": {
        "tags": ["Mint Details"],
        "summary": "Get meme token distribution",
        "description": "Retrieves the distribution details of a meme token including minter and vault balances",
        "operationId": "getMemeTokenDistribution",
        "parameters": [
          {
            "name": "memeId",
            "in": "path",
            "required": true,
            "description": "The meme ID to query",
            "schema": {
              "type": "string",
              "example": "meme_abc123"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Distribution retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MemeTokenDistributionResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid meme ID",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "404": {
            "description": "Meme token not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/meme/meme-token-balances": {
      "post": {
        "tags": ["Mint Details"],
        "summary": "Get multiple meme token balances",
        "description": "Retrieves balance information for multiple meme tokens",
        "operationId": "getAllMemeTokenBalances",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MemeTokenBalancesRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Balances retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MemeTokenBalancesResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request body",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/meme/meme-token-recent/{memeId}": {
      "get": {
        "tags": ["Mint Details"],
        "summary": "Get recent mint distribution",
        "description": "Retrieves formatted distribution details for a recently minted token",
        "operationId": "getRecentMintDistribution",
        "parameters": [
          {
            "name": "memeId",
            "in": "path",
            "required": true,
            "description": "The meme ID to query",
            "schema": {
              "type": "string",
              "example": "meme_abc123"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Distribution retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RecentMintDistributionResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid meme ID",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "404": {
            "description": "Meme token not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/meme/all-minted-tokens": {
      "get": {
        "tags": ["Mint Details"],
        "summary": "Get all minted tokens",
        "description": "Retrieves a list of all meme tokens that have been minted across all users",
        "operationId": "getAllMintedTokens",
        "responses": {
          "200": {
            "description": "All minted tokens retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AllMintedTokensResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/amm/create-amm-pool": {
      "post": {
        "tags": ["AMM Pool"],
        "summary": "Initialize AMM pool",
        "description": "Creates a new Automated Market Maker liquidity pool for a token with initial SOL and token deposits",
        "operationId": "createAmmPool",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateAmmPoolRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "AMM pool created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AmmPoolResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request - missing required fields",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/amm/add-liquidity": {
      "post": {
        "tags": ["AMM Pool"],
        "summary": "Add liquidity to pool",
        "description": "Adds liquidity to an existing AMM pool by depositing SOL and tokens proportionally",
        "operationId": "addLiquidity",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddLiquidityRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Liquidity added successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LiquidityResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request - missing required fields",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/amm/pool-info/{tokenMint}": {
      "get": {
        "tags": ["AMM Pool"],
        "summary": "Get pool information",
        "description": "Retrieves detailed information about an AMM pool including reserves, LP supply, and pool addresses",
        "operationId": "getPoolInfo",
        "parameters": [
          {
            "name": "tokenMint",
            "in": "path",
            "required": true,
            "description": "The token mint address of the pool",
            "schema": {
              "type": "string",
              "example": "EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Pool information retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PoolInfoResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid token mint address",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "404": {
            "description": "Pool not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "InitializeProtocolRequest": {
        "type": "object",
        "properties": {
          "adminPublicKey": {
            "type": "string",
            "description": "The admin's public key",
            "example": "5ZWj7a1f8tWkjBESHKgrLmXshuXxqeY9SYcfbshpAqPG"
          }
        },
        "required": ["adminPublicKey"]
      },
      "ResetProtocolRequest": {
        "type": "object",
        "properties": {
          "adminPublicKey": {
            "type": "string",
            "description": "The admin's public key",
            "example": "5ZWj7a1f8tWkjBESHKgrLmXshuXxqeY9SYcfbshpAqPG"
          }
        },
        "required": ["adminPublicKey"]
      },
      "MintMemeRequest": {
        "type": "object",
        "properties": {
          "memeId": {
            "type": "string",
            "description": "Unique identifier for the meme",
            "example": "meme_abc123"
          },
          "minterPublicKey": {
            "type": "string",
            "description": "The minter's public key",
            "example": "5ZWj7a1f8tWkjBESHKgrLmXshuXxqeY9SYcfbshpAqPG"
          }
        },
        "required": ["memeId", "minterPublicKey"]
      },
      "CreateAtaRequest": {
        "type": "object",
        "properties": {
          "mint": {
            "type": "string",
            "description": "The token mint address",
            "example": "EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v"
          },
          "owner": {
            "type": "string",
            "description": "The owner's public key",
            "example": "5ZWj7a1f8tWkjBESHKgrLmXshuXxqeY9SYcfbshpAqPG"
          }
        },
        "required": ["mint", "owner"]
      },
      "TransactionResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "example": true
          },
          "message": {
            "type": "string",
            "example": "Transaction created successfully"
          },
          "data": {
            "type": "object",
            "properties": {
              "transaction": {
                "type": "string",
                "description": "Base64 encoded unsigned transaction",
                "example": "AQAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAABAAEDArczbMIA..."
              },
              "message": {
                "type": "string",
                "example": "Please sign and send this transaction"
              }
            }
          }
        },
        "required": ["success", "data"]
      },
      "TokenBalanceResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "example": true
          },
          "data": {
            "type": "object",
            "properties": {
              "balance": {
                "type": "string",
                "description": "Raw balance amount",
                "example": "1000000000000000000"
              },
              "balanceNumber": {
                "type": "number",
                "description": "Balance as a number",
                "example": 1000000000000000000
              },
              "decimals": {
                "type": "integer",
                "description": "Number of decimals",
                "example": 9
              },
              "mint": {
                "type": "object",
                "description": "Token mint public key object"
              }
            },
            "required": ["balance", "balanceNumber", "decimals", "mint"]
          }
        },
        "required": ["success", "data"]
      },
      "MemeTokenDistributionResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "example": true
          },
          "data": {
            "type": "object",
            "properties": {
              "mint": {
                "type": "object",
                "description": "Token mint public key"
              },
              "totalSupply": {
                "type": "string",
                "example": "1000000000000000000"
              },
              "minterBalance": {
                "type": "object",
                "nullable": true,
                "properties": {
                  "address": {
                    "type": "object",
                    "description": "Minter's token account address"
                  },
                  "balance": {
                    "type": "string",
                    "example": "20000000000000000"
                  },
                  "balanceNumber": {
                    "type": "number",
                    "example": 20000000000000000
                  },
                  "percentage": {
                    "type": "number",
                    "example": 2
                  }
                }
              },
              "vaultBalance": {
                "type": "object",
                "nullable": true,
                "properties": {
                  "address": {
                    "type": "object",
                    "description": "Vault's token account address"
                  },
                  "balance": {
                    "type": "string",
                    "example": "980000000000000000"
                  },
                  "balanceNumber": {
                    "type": "number",
                    "example": 980000000000000000
                  },
                  "percentage": {
                    "type": "number",
                    "example": 98
                  }
                }
              },
              "distributionSummary": {
                "type": "object",
                "properties": {
                  "totalDistributed": {
                    "type": "number",
                    "example": 1000000000000000000
                  },
                  "minterShare": {
                    "type": "number",
                    "example": 2
                  },
                  "vaultShare": {
                    "type": "number",
                    "example": 98
                  },
                  "isCorrectDistribution": {
                    "type": "boolean",
                    "example": true
                  }
                }
              }
            }
          }
        },
        "required": ["success", "data"]
      },
      "MemeTokenBalancesRequest": {
        "type": "object",
        "properties": {
          "memeIds": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["meme_abc123", "meme_def456"]
          }
        },
        "required": ["memeIds"]
      },
      "MemeTokenBalancesResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "example": true
          },
          "data": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "memeId": {
                  "type": "string",
                  "example": "meme_abc123"
                },
                "memeIdHex": {
                  "type": "string",
                  "example": "6d656d655f616263313233"
                },
                "distribution": {
                  "type": "object",
                  "description": "Distribution details (same structure as MemeTokenDistributionResponse)"
                }
              }
            }
          }
        },
        "required": ["success", "data"]
      },
      "RecentMintDistributionResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "example": true
          },
          "data": {
            "type": "object",
            "properties": {
              "mint": {
                "type": "string",
                "description": "Token mint address",
                "example": "EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v"
              },
              "totalSupply": {
                "type": "string",
                "example": "1000000000.0"
              },
              "minter": {
                "type": "object",
                "nullable": true,
                "properties": {
                  "address": {
                    "type": "string",
                    "example": "HN7cABqLq46Es1jh92dQQisAq662SmxELLLsHHe4YWrH"
                  },
                  "balance": {
                    "type": "string",
                    "example": "20000000.0"
                  },
                  "percentage": {
                    "type": "string",
                    "example": "2.0000%"
                  }
                }
              },
              "vault": {
                "type": "object",
                "nullable": true,
                "properties": {
                  "address": {
                    "type": "string",
                    "example": "J3dxNj7nDRRqRRXuEMynDG57DkZK4jYRuv3Garmb1i99"
                  },
                  "balance": {
                    "type": "string",
                    "example": "980000000.0"
                  },
                  "percentage": {
                    "type": "string",
                    "example": "98.0000%"
                  }
                }
              },
              "summary": {
                "type": "object",
                "properties": {
                  "totalDistributed": {
                    "type": "string",
                    "example": "1000000000.0"
                  },
                  "isCorrect": {
                    "type": "boolean",
                    "example": true
                  },
                  "minterShare": {
                    "type": "string",
                    "example": "2.0000%"
                  },
                  "vaultShare": {
                    "type": "string",
                    "example": "98.0000%"
                  }
                }
              }
            }
          }
        },
        "required": ["success", "data"]
      },
      "AllMintedTokensResponse": {
        "type": "object",
        "properties": {
          "success": {
